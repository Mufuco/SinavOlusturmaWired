@model List<Exam>
@{
    ViewData["Title"] = "ExamList";
    Layout = "~/Views/Shared/_LayoutRe.cshtml";
}
@if(Model.Count()!=0){
<table class="table">
    <thead>
        <tr>
            <th scope="col">Başlık</th>
            <th scope="col">Tarih</th>
            <th scope="col"></th>

        </tr>
    </thead>
    <tbody>
        
        @foreach (var item in Model)
        {
            <tr>

                <td><a href="/ExamQuestion/SolveExam/@item.Id/">@item.ExamTitle</a></td>
                <td><a>@item.CreatedDate</a></td>
                <td> <a href="/Exam/ExamDelete/@item.Id" class="btn btn-danger">Sil</a></td>
                   
            </tr>
        }
            
    </tbody>
</table>

}
else{
    <h1>Hiç sınav oluşturulmamış!</h1>
}